      <xsd:enumeration value="Face" />
      <xsd:enumeration value="Text" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="rastinfo" type="rastinfoType" />
  <xsd:complexType name="rastinfoType">
    <xsd:sequence>
      <xsd:element ref="rasttype">
        <xsd:annotation>
          <xsd:documentation>3.4.1  Raster Object Type -- raster spatial objects used to locate
