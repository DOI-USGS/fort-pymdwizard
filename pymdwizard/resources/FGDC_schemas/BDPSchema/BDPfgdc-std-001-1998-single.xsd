      </xsd:choice>
      <xsd:element ref="latprjo" />
      <xsd:element ref="feast" />
      <xsd:element ref="fnorth" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="orthogr" type="orthogrType" />
